
@{
                /**/

                ViewBag.Title = "GetByCarEverything";
}

<h2>GetByCarEverything</h2>

@if (ViewBag.Cars == null)
{
    <h1>No Search Result was found</h1>
}
else
{
    <table class="table table-bordered table-striped">
        <!--onload="~/Home/Draw/"-->
        <tr>
            <th>Make</th>
            <th>Model</th>
            <th>Year</th>
            <th>Color</th>

        </tr>
        @for (int i = 0; i < ViewBag.Cars.Count; i++)
        {
            var Car = ViewBag.Cars[i];
            <tr>
                <td>
                    @Car["Make"]
                </td>
                <td>
                    @Car["Model"]
                </td>
                <td>
                    @Car["Year"]
                </td>
                <td>
                    @Car["Color"]
                </td>
            </tr>
        }
    </table>
}
